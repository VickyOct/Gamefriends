<div class="card">

  <div class="card-body">
    <h5 class="card-title"><%= "Title： #{note.title}" %></h5>
    <p class="card-text"><%= "Note： #{note.note}" %></p>
    <p class="card-text"><%= "Score： #{note.score}" %></p>
    <p class="card-text"><%= "(#{time_ago_in_words note.created_at} ago)" %></p>

    <div >
      <%= button_to "Delete Note", [@game, note], method: :delete%>
    </div>
  </div>

  <!--  <div><%#= simple_format note.note %></div>-->
</div>